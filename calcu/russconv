	cmp bl,06h
	jge label2
	mov ah,2
	mov dl,bl 
	add dl,30h
	int 21h
	jmp cont 
label2:
	xor dx,dx 
	xor ax,ax
	xor bh,bh 
	mov ax,bx 
	mov bx,06h 
	div bx 
	mov bx,ax 
	mov cx,dx 
	mov ah,2
	mov dl,bl 
	add dl,30h
	int 21h
	mov dl,cl 
	add dl,30h
	int 21h
cont:
	tryagain tryagain1
	cmp al,'Y'
	je label1
	cmp al,'y'
	je label1
	cmp al,'N'
	je start
	cmp al,'n'
	je start
	jmp cont 	
endm
