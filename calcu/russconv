userinterface2 macro a,b,c,d 
	clearscreen
	setcursorpos 06h,23h
	cout a 
	setcursorpos 07h,22h
	cout b 
	setcursorpos 09h,1dh
	cout c 
	setcursorpos 0ah,1dh
	cout d
endm
;---------------
binarydecimal macro
local label1,label2,cont
label1:
	userinterface2 header,bindechead,inpbin,outdec
	setcursorpos 09h,3eh
	binaryinput
	setcursorpos 0ah,37h
	cmp bl,0ah 
	jge label2 
	mov ah,2
	mov dl,bl 
	add dl,30h
	int 21h
	jmp cont
label2:
	mov ah,2
	mov dl,31h
	int 21h
	mov bh,0ah
	sub bl,bh
	mov dl,bl 
	add dl,30h
	int 21h
cont:
	tryagain tryagain1
	cmp al,'Y'
	je label1
	cmp al,'y'
	je label1
	cmp al,'N'
	je start
	cmp al,'n'
	je start
	jmp cont 
endm
;---------------
binaryinput macro 
    mov   ah,1
    int   21h     

    sub al,30h
    shl al,3
    mov bl,al

    mov ah,1
    int 21h

    sub al,30h
    shl al,2
    add bl,al

    mov ah,1
    int 21h

    sub al,30h
    shl al,1
    add bl,al

    mov ah,1
    int 21h

    sub al,30h
    add bl,al
    xor bh,bh 
endm
;---------------
tryagain macro a 
	setcursorpos 0bh,1dh
	cout a
	pick
endm
;---------------
binaryhexadecimal macro
local label,label2,cont
label1:
	userinterface2 header,binhexhead,inpbin,outhex
	setcursorpos 09h,3eh
	binaryinput
	setcursorpos 0ah,3bh
	cmp bl,0ah
	jge label2
	mov ah,2
	mov dl,bl
	add dl,30h
	int 21h
	jmp cont
label2:
	mov ah,2
	mov dl,bl
	add dl,37h
	int 21h
cont:
	tryagain tryagain1
	cmp al,'Y'
	je label1
	cmp al,'y'
	je label1
	cmp al,'N'
	je start
	cmp al,'n'
	je start
	jmp cont 	
endm
