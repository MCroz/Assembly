code segment
assume cs:code;ds:code
org 100h
.386
;-----------------
clearscreen macro
	pushad
	mov ax,0600h
	mov bh,07h
	mov cx,0000h
	mov dx,184fh
	int 10h 
	popad
endm
;----------------
setcursorpos macro x,y 
	pushad
	mov ah,02
	mov bh,0
	mov dh,x
	mov dl,y 
	int 10h 
	popad
endm
;---------------
cout macro string
	pushad
	lea dx,string
	mov ah,9
	int 21h
	popad
endm
;---------------
userinterface macro a,b,c,d,e,f,g
	clearscreen
	setcursorpos 06h,21h
	cout a 
	setcursorpos 0ah,1dh
	cout b
	setcursorpos 0bh,1dh
	cout c
	setcursorpos 0ch,1dh
	cout d
	setcursorpos 0dh,1dh
	cout e
	setcursorpos 0eh,1dh
	cout f
	setcursorpos 10h,1ch
	cout g
endm
;---------------
pick macro
	mov ah,1
	int 21h
endm
;---------------
binarymenu macro
local label1,label2,label3,label4,label5,wronginput
wronginput:
	userinterface cbin,cbindec,cbinhex,cbinbase6,cbinbase12,cexit,cchoice
	pick
	cmp al,'1'
	je label1
	cmp al,'2'
	je label2
	cmp al,'3'
	je label3
	cmp al,'4'
	je label4
	cmp al,'5'
	je label5
	jmp wronginput
label1:
	binarydecimal
label2:
	binaryhexadecimal
label3:
	binarybase6
label4:
	binarybase12
label5:
 	jmp start
endm
;---------------
