decimalinput macro
	mov ah,1
	int 21h
	sub al,30h
	xor ah,ah
	mov bx,0ah
	mul bx
	mov bx,ax
	mov ah,1
	int 21h
	sub al,30h
	xor ah,ah 
	add bx,ax
endm
