hexadecimalinput macro
local label1,label2,end,numerical1,numerical2,small1,small2,capital1,capital2
label1:
	mov ah,1
	int 21h
	cmp al,'9'
	jle numerical1
	cmp al,'F'
	jle capital1
	cmp al,'f'
	jle small1
	jmp label1
numerical1:
	sub al,30h
	xor ah,ah
	mov bx,10h 
	mul bx 
	mov bx,ax
	jmp label2
capital1:
	sub al,37h
	xor ah,ah
	mov bx,10h
	mul bx 
	mov bx,ax
	jmp label2
small1:
	sub al,57h
	xor ah,ah
	mov bx,10h
	mul bx 
	mov bx,ax
label2:
	mov ah,1
	int 21h
	cmp al,'9'
	jle numerical2
	cmp al,'F'
	jle capital2
	cmp al,'f'
	jle small2
	jmp label2
numerical2:
	sub al,30h
	xor ah,ah
	add bx,ax
	jmp end
capital2:
	sub al,37h
	xor ah,ah
	add bx,ax
	jmp end 
small2:
	sub al,57h
	xor ah,ah
	add bx,ax
end:	
endm
