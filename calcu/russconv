base6hexadecimal macro
local label1,label2,label3,cont
label1:
	userinterface2 header,base6hexhead,inpbase6,outhex
	setcursorpos 09h,39h
	base6input
	setcursorpos 0ah,3bh
	mov ax,bx 
	mov bx,10h
	xor dx,dx 
	div bx 
	mov bx,ax 
	mov cx,dx
	mov ah,2
	mov dl,bl
	cmp dl,0
	je label2
	add dl,30h
	int 21h
label2:
	mov dl,cl 
	cmp dl,0ah
	jge label3
	add dl,30h
	int 21h
	jmp cont 
label3:
	add dl,37h
	int 21h
cont:
	tryagain tryagain1
	cmp al,'Y'
	je label1
	cmp al,'y'
	je label1
	cmp al,'N'
	je start
	cmp al,'n'
	je start
	jmp cont 
