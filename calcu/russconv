start:

	clearscreen
	setcursorpos 6,1fh
	cout mainmenu

	setcursorpos 0ah,1dh
	cout mbin

	setcursorpos 0bh,1dh
	cout mdec

	setcursorpos 0ch,1dh 
	cout mhex 

	setcursorpos 0dh,1dh
	cout mbase6

	setcursorpos 0eh,1dh
	cout mbase12 

	setcursorpos 0fh,1dh
	cout mexit

mc:
	setcursorpos 12h,1ch
	cout mchoice

	pick
	cmp al,'1'
	je one
	cmp al,'2'
	je two
	cmp al,'3'
	je three
	cmp al,'4'
	je four
	cmp al,'5'
	je five
	cmp al,'6'
	je six
	jmp mc

one:
	binarymenu
two:
	decimalmenu
three:
	hexadecimalmenu
four:
	base6menu
five:
	base12menu
six:
 	int 20h

code ends
end begins
