	int 21h
	mov ch,cl 
	and ch,04h
	shr ch,2
	mov ah,2
	mov dl,ch
	add dl,30h
	int 21h
	mov ch,cl 
	and ch,02h
	shr ch,1
	mov ah,2
	mov dl,ch 
	add dl,30h
	int 21h
	mov ch,cl
	and ch,1
	mov ah,2
	mov dl,ch
	add dl,30h
	int 21h	
cont:
	tryagain tryagain1
	cmp al,'Y'
	je label1
	cmp al,'y'
	je label1
	cmp al,'N'
	je start
	cmp al,'n'
	je start
	jmp cont 	
endm
