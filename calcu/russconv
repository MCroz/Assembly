	cmp bx,0ah
	jge label2
	mov ah,2
	mov dl,bl
	add dl,30h
	int 21h
	jmp cont
label2:
	mov ax,bx 
	mov bx,0ah
	xor dx,dx 
	div bx 
	mov bx,ax 
	mov cx,dx 
	cmp bx,14h
	jge label3
	cmp bx,0ah
	jge label4
	mov ah,2
	mov dl,bl 
	add dl,30h
	int 21h
	mov dl,cl 
	add dl,30h
	int 21h
	jmp cont
label3:
	sub bx,14h
	mov ah,2
	mov dl,32h
	int 21h
	mov dl,bl 
	add dl,30h
	int 21h
	mov dl,cl 
	add dl,30h
	int 21h
	jmp cont
label4:
	sub bx,0ah
	mov ah,2
	mov dl,31h
	int 21h
	mov dl,bl 
	add dl,30h
	int 21h
	mov dl,cl 
	add dl,30h
	int 21h
cont:
	tryagain tryagain1
	cmp al,'Y'
	je label1
	cmp al,'y'
	je label1
	cmp al,'N'
	je start
	cmp al,'n'
	je start
	jmp cont 
endm
;---------------
base12base6 macro
local label1,label2,label3,label4,cont
label1:
	userinterface2 header,base12base6head,inpbase12,outbase6
	setcursorpos 09h,3bh
	base12input
	setcursorpos 0ah,35h
	cmp bx,06h 
	jge label2
	mov ah,2
	mov dl,bl
	add dl,30h
	int 21h
	jmp cont
label2:
	mov ax,bx 
	mov bx,06h 
	xor dx,dx 
	div bx 
	mov bx,ax 
	mov cx,dx 
	cmp bx,06h
	jge label3
	mov ah,2
	mov dl,bl
	add dl,30h
	int 21h
	mov dl,cl 
	add dl,30h 
	int 21h
	jmp cont 
label3:
	mov ax,bx 
	mov bx,06h 
	xor dx,dx 
	div bx
	push cx 
	mov bx,ax 
	mov cx,dx
	cmp bx,06h 
	jge label4 
	mov ah,2
	mov dl,bl 
	add dl,30h
	int 21h
	mov dl,cl 
	add dl,30h
	int 21h
	pop cx 
	mov dl,cl
	add dl,30h
	int 21h
	jmp cont
label4:
	mov ax,bx 
	mov bx,06h 
	xor dx,dx 
	div bx 
	push cx 
	mov bx,ax 
	mov cx,dx 
	mov ah,2
	mov dl,bl
	add dl,30h
	int 21h
	mov dl,cl 
	add dl,30h
	int 21h
	pop cx 
	mov dl,cl
	add dl,30h
	int 21h
	pop cx 
	mov dl,cl 
	add dl,30h
	int 21h

cont:
	tryagain tryagain1
	cmp al,'Y'
	je label1
	cmp al,'y'
	je label1
	cmp al,'N'
	je start
	cmp al,'n'
	je start
	jmp cont 
endm
