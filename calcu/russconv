hexadecimalbinary macro
local label1,cont
label1:
	userinterface2 header,hexbinhead,inphex,outbin
	setcursorpos 09h,3fh
	hexadecimalinput
	setcursorpos 0ah,36h
	xor dx,dx
	mov ax,bx 
	mov bx,10h
	div bx 
	mov bx,ax 
	mov cx,dx
	mov bh,bl
	shr bh,3
	mov ah,2
	mov dl,bh
	add dl,30h
	int 21h
	mov bh,bl 
	and bh,04h
	shr bh,2
	mov ah,2
	mov dl,bh
	add dl,30h
	int 21h
	mov bh,bl 
	and bh,02h
	shr bh,1
	mov ah,2
	mov dl,bh 
	add dl,30h
	int 21h
	mov bh,bl
	and bh,1
	mov ah,2
	mov dl,bh
	add dl,30h
	int 21h
	mov ah,2
	mov dl,20h
	int 21h

	mov ch,cl
	shr ch,3
	mov ah,2
	mov dl,ch
	add dl,30h
	int 21h
	mov ch,cl 
	and ch,04h
	shr ch,2
	mov ah,2
	mov dl,ch
	add dl,30h
	int 21h
	mov ch,cl 
	and ch,02h
	shr ch,1
	mov ah,2
	mov dl,ch 
	add dl,30h
	int 21h
	mov ch,cl
	and ch,1
	mov ah,2
	mov dl,ch
	add dl,30h
	int 21h	
cont:
	tryagain tryagain1
	cmp al,'Y'
	je label1
	cmp al,'y'
	je label1
	cmp al,'N'
	je start
	cmp al,'n'
	je start
	jmp cont 	
endm
