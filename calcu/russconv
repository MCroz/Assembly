;---------------
base12input macro
local label1,label2,end,numerical1,numerical2,small1,small2,capital1,capital2
label1:
	mov ah,1
	int 21h
	cmp al,'9'
	jle numerical1
	cmp al,'B'
	jle capital1
	cmp al,'b'
	jle small1
	jmp label1
numerical1:
	sub al,30h
	xor ah,ah
	mov bx,0ch 
	mul bx 
	mov bx,ax
	jmp label2
capital1:
	sub al,37h
	xor ah,ah
	mov bx,0ch
	mul bx 
	mov bx,ax
	jmp label2
small1:
	sub al,57h
	xor ah,ah
	mov bx,0ch
	mul bx 
	mov bx,ax
label2:
	mov ah,1
	int 21h
	cmp al,'9'
	jle numerical2
	cmp al,'B'
	jle capital2
	cmp al,'b'
	jle small2
	jmp label2
numerical2:
	sub al,30h
	xor ah,ah
	add bx,ax
	jmp end
capital2:
	sub al,37h
	xor ah,ah
	add bx,ax
	jmp end 
small2:
	sub al,57h
	xor ah,ah
	add bx,ax
end:	
endm
;---------------
base12binary macro
local label1,cont
label1:
	userinterface2 header,base12binhead,inpbase12,outbin
	setcursorpos 09h,39h
	base12input
	setcursorpos 0ah,36h
	xor dx,dx
	mov ax,bx 
	mov bx,10h
	div bx 
	mov bx,ax 
	mov cx,dx
	mov bh,bl
	shr bh,3
	mov ah,2
	mov dl,bh
	add dl,30h
	int 21h
	mov bh,bl 
	and bh,04h
	shr bh,2
	mov ah,2
	mov dl,bh
	add dl,30h
	int 21h
	mov bh,bl 
	and bh,02h
	shr bh,1
	mov ah,2
	mov dl,bh 
	add dl,30h
	int 21h
	mov bh,bl
	and bh,1
	mov ah,2
	mov dl,bh
	add dl,30h
	int 21h
	mov ah,2
	mov dl,20h
	int 21h

	mov ch,cl
	shr ch,3
	mov ah,2
	mov dl,ch
	add dl,30h
	int 21h
	mov ch,cl 
	and ch,04h
	shr ch,2
	mov ah,2
	mov dl,ch
	add dl,30h
	int 21h
	mov ch,cl 
	and ch,02h
	shr ch,1
	mov ah,2
	mov dl,ch 
	add dl,30h
	int 21h
	mov ch,cl
	and ch,1
	mov ah,2
	mov dl,ch
	add dl,30h
	int 21h	

cont:
	tryagain tryagain1
	cmp al,'Y'
	je label1
	cmp al,'y'
	je label1
	cmp al,'N'
	je start
	cmp al,'n'
	je start
	jmp cont 	
endm
